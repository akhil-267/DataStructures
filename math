#include<bits/stdc++.h>
using namespace std;

typedef vector<int> vi;
typedef vector<int,int> vv;
typedef pair<int,int> pi;
#define F first
#define S second
#define PB push_back
#define MP make_pair
#define REP(i,a,b) for(int i=a;i>=b;i--)
#define REPA(i,a,b) for(int i=a;i<b;i++)
#define SQ(a) (a)*(a)


int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);
    
    int t;
    cin>>t;
    while(t--)
    {
    	int n,m,count=0;
    	cin>>n>>m;
    	
    	cout<<gcd(n,m)<<endl;
// gcd gives the no of distributed childrens.. 8->2*4  12->3*4   
//2 is four times and 3 is four times that means we can distribute to four children
    	
    }
    
    
}
    
   
